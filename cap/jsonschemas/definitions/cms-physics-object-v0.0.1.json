{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "id": "physics_object",
    "type": "object",
    "properties": {
        "object": {
            "title": "Object",
            "type": "string",
            "enum": [
                "",
                "electron",
                "muon",
                "tau",
                "jet",
                "bjet",
                "photon",
                "track",
                "vertex",
                "MET",
                "HT"
            ]
        },
        "electron_type": {
            "type": "string",
            "title": "Electron type",
            "enum": [
                "GsfElectron"
            ]
        },
        "muon_type": {
            "type": "string",
            "title": "Muon type",
            "enum": [
                "PFMuon",
                "GlobalMuon",
                "TrackerMuon"
            ]
        },
        "tau_type": {
            "type": "string",
            "title": "Tau type",
            "enum": [
                "PFTau"
            ]
        },
        "jet_type": {
            "type": "string",
            "title": "Jet type",
            "enum": [
                "AK4PF",
                "AK5PF",
                "AK6PF",
                "AK7PF",
                "AK8PF",
                "AK4Calo",
                "AK5Calo",
                "AK6Calo",
                "AK7Calo",
                "AK8Calo",
                "KT4PF",
                "KT5PF",
                "KT6PF",
                "KT7PF",
                "KT8PF",
                "KT4Calo",
                "KT5Calo",
                "KT6Calo",
                "KT7Calo",
                "KT8Calo"
            ]
        },
        "jet_corrections": {
            "type": "string",
            "title": "Jet Corrections",
            "enum": [
                "JetCorrections"
            ]
        },
        "photon_type": {
            "type": "string",
            "title": "Photon type",
            "enum": [
                "Photon",
                "Conversion"
            ]
        },
        "met_type": {
            "type": "string",
            "title": "MET type",
            "enum": [
                "PFMET",
                "CaloMET"
            ]
        },
        "met_pf_corrections": {
            "type": "string",
            "title": "MET Corrections",
            "enum": [
                "Type0PFCorrections",
                "Type1PFCorrections",
                "Type2PFCorrections"
            ]
        },
        "met_calo_corrections": {
            "type": "string",
            "title": "MET Corrections",
            "enum": [
                "Type0CaloCorrections",
                "Type1CaloCorrections",
                "Type2CaloCorrections"
            ]
        },
        "track_type": {
            "type": "string",
            "title": "Track type",
            "enum": [
                "Heavy-Ion",
                "pp"
            ]
        },
        "sel_criteria": {
            "type": "string",
            "title": "Selection Criteria",
            "enum": ["tight", "medium", "loose", "other"]
        },
        "isolation": {
            "title": "Isolation",
            "type": "object",
            "properties": {
                "notracks": {
                    "title": "no tracks",
                    "type": "object",
                    "properties": {
                        "pTg": {
                            "type":"string",
                            "pattern": "^[\\d\\.]+$",
                            "title":"pT >"
                        },
                        "deltaRs": {
                            "type":"string",
                            "pattern": "^[\\d\\.]+$",
                            "title":"ÎR <"
                        }
                    }
                },
                "calorimeter": {
                    "title": "calorimeter",
                    "type": "object",
                    "properties": {
                        "pTs": {
                            "type":"string",
                            "pattern": "^[\\d\\.]+$",
                            "title":"pT <"
                        },
                        "deltaRs": {
                            "type":"string",
                            "pattern": "^[\\d\\.]+$",
                            "title":"ÎR <"
                        }
                    }
                }
            }
        },
        "number": {
            "title": "Number of particles",
            "type": "object",
            "properties": {
                "sign": {
                    "type": "string",
                    "title": "<, >, =, <=, >=",
                    "enum": [
                        "<",
                        ">",
                        "=",
                        "<=",
                        ">="
                    ],
                    "default": "="
                },
                "number": {
                    "type":"string",
                    "pattern": "^[\\d\\.]+$",
                    "title":"Number"
                }
            }
        },
        "number_vertex": {
            "title": "Number",
            "type": "object",
            "properties": {
                "sign": {
                    "type": "string",
                    "title": "=",
                    "enum": [
                        "="
                    ]
                },
                "number": {
                    "type":"string",
                    "pattern": "^[\\d\\.]+$",
                    "title":"Number"
                }
            }
        },
        "number_tracks": {
            "title": "Number of Tracks",
            "type": "object",
            "properties": {
                "number": {
                    "type":"string",
                    "pattern": "^[\\d\\.]+$",
                    "title":">"
                }
            }
        },
        "electron_discriminator": {
            "title": "Discriminator",
            "type": "object",
            "properties": {
                "tag": {
                    "type": "string",
                    "title": "Tag",
                    "enum":[  
                        "other"
                    ]
                },
                "custom_tag": {
                    "type": "string",
                    "title": " "
                },
                "value": {
                    "type":"string",
                    "pattern": "^[\\d\\.]+$",
                    "title":"Value"
                }
            }
        },
        "muon_discriminator": {
            "title": "Discriminator",
            "type": "object",
            "properties": {
                "tag": {
                    "type": "string",
                    "title": "Tag",
                    "enum": [
                        "other"
                    ]
                },
                "custom_tag": {
                    "type": "string",
                    "title": " "
                },
                "value": {
                    "type":"string",
                    "pattern": "^[\\d\\.]+$",
                    "title":"Value"
                }
            }
        },
        "tau_discriminator": {
            "title": "Discriminator",
            "type": "object",
            "properties": {
                "tag": {
                    "type": "string",
                    "title": "Tag",
                    "enum": [
                        "AgainstElectronLooseMVA",
                        "other"
                    ]
                },
                "custom_tag": {
                    "type": "string",
                    "title": " "
                },
                "value": {
                    "type":"string",
                    "pattern": "^[\\d\\.]+$",
                    "title":"Value"
                }
            }
        },
        "jet_discriminator": {
            "title": "Discriminator",
            "type": "object",
            "properties": {
                "tag": {
                    "type": "string",
                    "title": "Tag",
                    "enum": [
                        "other"
                    ]
                },
                "custom_tag": {
                    "type": "string",
                    "title": " "
                },
                "value": {
                    "type":"string",
                    "pattern": "^[\\d\\.]+$",
                    "title":"Value"
                }
            }
        },
        "bjet_discriminator": {
            "title": "Discriminator",
            "type": "object",
            "properties": {
                "tag": {
                    "type": "string",
                    "title": "Tag",
                    "enum": [
                        "trackCountingHighPurBJetTags",
                        "trackCountingHighEffBJetTags",
                        "simpleSecondaryVertexHighEffBJetTags",
                        "combinedSecondaryVertexBJetTags",
                        "jetProbabilityBJetTags",
                        "other"
                    ]
                },
                "custom_tag": {
                    "type": "string",
                    "title": " "
                },
                "value": {
                    "type":"string",
                    "pattern": "^[\\d\\.]+$",
                    "title":"Value"
                }
            }
        },
        "photon_discriminator": {
            "title": "Discriminator",
            "type": "object",
            "properties": {
                "tag": {
                    "type": "string",
                    "title": "Tag",
                    "enum": [
                        "other"
                    ]
                },
                "custom_tag": {
                    "type": "string",
                    "title": " "
                },
                "value": {
                    "type":"string",
                    "pattern": "^[\\d\\.]+$",
                    "title": "Value"
                }
            }
        },
        "pt_cut": {
            "title": "pT Cuts",
            "type": "array",
            "default": [],
            "items": {
                "type": "object",
                "properties": {
                    "sign": {
                        "type": "string",
                        "title": "<, >, =",
                        "enum": [
                            ">",
                            "=",
                            "<"
                        ],
                        "default": ">"
                    },
                    "number": {
                        "type":"string",
                        "pattern": "^[\\d\\.]+$",
                        "title": "GeV"
                    }
                }
            }
        },
        "eta_cut": {
            "title": "|Î·| Cuts",
            "type": "array",
            "default": [],
            "items": {
                "type": "object",
                "properties": {
                    "sign": {
                        "type": "string",
                        "title": "<, >, =",
                        "enum": [
                            ">",
                            "=",
                            "<"
                        ],
                        "default": ">"
                    },
                    "number": {
                        "type": "string",
                        "title": " "
                    },
                    "other_number": {
                        "type":"string",
                        "pattern": "^[\\d\\.]+$",
                        "title": " "
                    }
                }
            }
        }
    }
}
