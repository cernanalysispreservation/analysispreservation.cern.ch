{
  "name": "cms-analysis",
  "version": "0.0.1",
  "fullname": "CMS Analysis",
  "experiment": "CMS",
  "is_indexed": true,
  "use_deposit_as_record": true,
  "deposit_schema": {
    "title": "CMS Analysis - Draft",
    "dependencies": {
      "analysis_reuse_mode": {
        "properties": {
          "main_measurements": {
            "items": {
              "$ref":
                "https://analysispreservation.cern.ch/schemas/deposits/records/cms-main-measurements-v0.0.1.json"
            },
            "title": "Main Measurements Workflows",
            "type": "array",
            "description":
              "Please provide information about the main measurements of your analysis",
            "id": "main_measurements"
          },
          "final_results": {
            "title": "Final Results",
            "type": "object",
            "properties": {
              "code_base": {
                "x-cap-file": {},
                "type": "string",
                "description": "Include your code for n-tuple production",
                "title": "Your Code"
              },
              "processing_steps": {
                "$ref":
                  "https://analysispreservation.cern.ch/schemas/deposits/records/cms-processing-v0.0.1.json"
              }
            },
            "description":
              "Please provide information necessary to generate final plots and tables for your analysis."
          },
          "systematic_uncertainties": {
            "items": {
              "type": "object",
              "properties": {
                "type": {
                  "enum": [
                    "associated_to_obj",
                    "associated_to_background_pred_methods",
                    "associated_to_mc_production",
                    "assocciated_to_luminosity",
                    "statement_about_most_important_systematics"
                  ],
                  "type": "string",
                  "title": "Type"
                },
                "physics_object": {
                  "type": "string",
                  "description":
                    "Choose from objects defined in main measurements",
                  "title": "Physics Object"
                },
                "statement": {
                  "type": "string",
                  "title": "Statement about the most important systematics"
                }
              }
            },
            "type": "array",
            "description": "Details on the systematic uncertainties",
            "title": "Systematic Uncertainties"
          },
          "auxiliary_measurements": {
            "items": {
              "type": "object",
              "properties": {
                "type": {
                  "enum": [
                    "trigger_efficiencies",
                    "background_measurements",
                    "systematic_uncertainties",
                    "mc_scale_factors",
                    "object_efficiencies",
                    "rates",
                    "acceptance_corrections",
                    "other"
                  ],
                  "type": "string",
                  "title": "Type"
                },
                "links": {
                  "items": {
                    "x-cap-file": {},
                    "type": "string"
                  },
                  "type": "array",
                  "description": "Code for auxiliary measurements",
                  "title": "Your code"
                }
              }
            },
            "type": "array",
            "description":
              "Provide details on auxiliary measurements used in the analysis",
            "title": "Auxiliary Measurements"
          },
          "background_estimations": {
            "items": {
              "$ref":
                "https://analysispreservation.cern.ch/schemas/deposits/records/cms-background-estimation-v0.0.1.json"
            },
            "type": "array",
            "description": "Details on the background estimation methods",
            "title": "Background Estimation"
          }
        }
      }
    },
    "additionalProperties": true,
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
      "statistical_treatment": {
        "type": "object",
        "properties": {
          "combine_datacard": {
            "x-cap-file": {},
            "type": "string",
            "description": "Add final combine datacards of the analysis",
            "title": "Combine Datacard File"
          },
          "statistics_questionnaire": {
            "properties": {
              "$ref": {
                "type": "string"
              }
            },
            "title": "Statistics Questionnare"
          }
        },
        "title": "Statistical Treatment"
      },
      "_buckets": {
        "$ref":
          "https://analysispreservation.cern.ch/schemas/deposits/records/buckets-v0.0.1.json"
      },
      "control_number": {
        "type": "string"
      },
      "_files": {
        "$ref":
          "https://analysispreservation.cern.ch/schemas/deposits/records/file-v0.0.1.json"
      },
      "basic_info": {
        "properties": {
          "cadi_id": {
            "pattern": "^[A-Z0-9]{3}-[0-9]{2}-[0-9]{3}$",
            "type": "string",
            "x-validate-cadi-id": {},
            "placeholder": "e.g. JME-10-107",
            "title": "CADI ID"
          },
          "abstract": {
            "type": "string",
            "title": "Abstract"
          },
          "ana_notes": {
            "items": {
              "pattern": "^AN-[0-9]{4}/[0-9]{3}$",
              "placeholder": "e.g. AN-2010-107",
              "type": "string"
            },
            "type": "array",
            "title": "Analysis Notes"
          },
          "conclusion": {
            "type": "string",
            "title": "Conclusion"
          }
        },
        "title": "Basic Information",
        "type": "object",
        "id": "basic_info",
        "description":
          "Please provide some information relevant for all parts of the Analysis here"
      },
      "analysis_reuse_mode": {
        "type": "string",
        "description":
          "please turn this mode on if you want to capture additional information about main and auxiliary measurements, systematic uncertainties, background estimates, final state particles",
        "title": "Full reproducibility mode"
      },
      "input_data": {
        "properties": {
          "mc_bg_datasets": {
            "items": {
              "placeholder": "Path",
              "x-validate-das-path": {},
              "type": "string"
            },
            "title": "Monte Carlo Background Datasets",
            "type": "array",
            "description": "Please list all Monte Carlo Background datasets",
            "id": "mc_bg_datasets"
          },
          "json_files": {
            "items": {
              "x-cap-file": {},
              "type": "string"
            },
            "type": "array",
            "title": "Official JSON files"
          },
          "primary_datasets": {
            "items": {
              "type": "object",
              "x-validate-cms-trigger": {},
              "properties": {
                "ranges": {
                  "type": "object",
                  "properties": {
                    "max": {
                      "type": "number",
                      "title": "Max"
                    },
                    "min": {
                      "type": "number",
                      "title": "Min"
                    }
                  },
                  "title": "Ranges"
                },
                "path": {
                  "type": "string",
                  "x-validate-das-path": {},
                  "title": "Path"
                },
                "year": {
                  "type": "string",
                  "title": "Year"
                },
                "triggers": {
                  "items": {
                    "type": "object",
                    "properties": {
                      "trigger": {
                        "type": "string",
                        "title": "Trigger"
                      },
                      "prescale": {
                        "type": "number",
                        "title": "Prescale"
                      }
                    }
                  },
                  "title": "Triggers",
                  "type": "array",
                  "description": "Add selection triggers here",
                  "id": "triggers"
                }
              }
            },
            "title": "Primary Datasets",
            "type": "array",
            "description": "Please list all primary datasets here",
            "id": "primary_datasets"
          },
          "mc_sig_datasets": {
            "items": {
              "placeholder": "Path",
              "x-validate-das-path": {},
              "type": "string"
            },
            "title": "Monte Carlo Signal Datasets",
            "type": "array",
            "description": "Please list all Monte Carlo Signal datasets",
            "id": "mc_sig_datasets"
          }
        },
        "title": "Input Data",
        "type": "object",
        "id": "input_data",
        "description":
          "Please list all datasets and triggers relevant for your analysis here"
      },
      "cadi_info": {
        "$ref":
          "https://analysispreservation.cern.ch/schemas/deposits/records/cms-cadi-info-v0.0.1.json"
      },
      "ntuples_production": {
        "items": {
          "properties": {
            "code_base": {
              "x-cap-file": {},
              "type": "string",
              "description": "Include your code for n-tuple production",
              "title": "Your Code"
            },
            "processing_steps": {
              "$ref":
                "https://analysispreservation.cern.ch/schemas/deposits/records/cms-processing-v0.0.1.json"
            }
          },
          "type": "object",
          "description": "Add instructions to run your code",
          "title": ""
        },
        "title": "N-tuples Production",
        "type": "array",
        "description":
          "Provide details on the intermediate n-tuples production",
        "id": "ntuples_production"
      },
      "_access": {
        "$ref":
          "https://analysispreservation.cern.ch/schemas/deposits/records/access-v0.0.1.json"
      },
      "_deposit": {
        "$ref":
          "https://analysispreservation.cern.ch/schemas/deposits/records/deposit-v0.0.1.json"
      },
      "additional_resources": {
        "title": "Additional Resources",
        "type": "object",
        "properties": {
          "comments": {
            "type": "string",
            "title": "Additional comment"
          },
          "presentations": {
            "items": {
              "type": "object",
              "properties": {
                "conference": {
                  "type": "string",
                  "title": "Conference/meeting"
                },
                "items": {
                  "x-cap-file": {},
                  "type": "string",
                  "title": "Presentation"
                }
              }
            },
            "title": "Presentations",
            "type": "array",
            "id": "presentations",
            "description": "Upload presentations about the analysis"
          },
          "documentations": {
            "items": {
              "x-cap-file": {},
              "type": "string"
            },
            "title": "Additional Documentations",
            "type": "array",
            "id": "documentations",
            "description": "Add any useful additional documentation"
          },
          "publications": {
            "items": {
              "$ref":
                "https://analysispreservation.cern.ch/schemas/deposits/records/publication-v0.0.1.json"
            },
            "title": "Publications",
            "type": "array",
            "id": "publications",
            "description": "Add references to related publications"
          },
          "keywords": {
            "type": "string",
            "id": "keywords",
            "title": "Keywords"
          },
          "internal_discussions": {
            "items": {
              "type": "string",
              "title": "URL"
            },
            "title": "Internal Discussions",
            "type": "array",
            "id": "internal-discussions",
            "description": "Link to hypernews thread"
          }
        },
        "description": "Add any useful additional documentation on the analysis"
      },
      "$ana_type": {
        "type": "string"
      },
      "$schema": {
        "type": "string"
      },
      "general_title": {
        "type": "string"
      },
      "_experiment": {
        "type": "string"
      },
      "_fetched_from": {
        "type": "string"
      },
      "_user_edited": {
        "type": "boolean"
      }
    }
  },
  "deposit_mapping": {
    "settings": {
      "analysis": {
        "analyzer": {
          "lowercase_whitespace_analyzer": {
            "type": "custom",
            "tokenizer": "whitespace",
            "filter": ["lowercase"]
          }
        }
      }
    },
    "mappings": {
      "cms-analysis-v0.0.1": {
        "_all": {
          "enabled": true,
          "analyzer": "lowercase_whitespace_analyzer"
        },
        "properties": {
          "_deposit": {
            "type": "object",
            "properties": {
              "created_by": {
                "type": "integer",
                "copy_to": "created_by"
              },
              "status": {
                "type": "text",
                "fields": {
                  "keyword": {
                    "type": "keyword"
                  }
                },
                "copy_to": "status"
              }
            }
          },
          "_experiment": {
            "type": "text"
          },
          "_fetched_from": {
            "type": "text",
            "analyzer": "lowercase_whitespace_analyzer",
            "copy_to": "fetched_from"
          },
          "_user_edited": {
            "type": "boolean",
            "copy_to": "user_edited"
          },
          "_created": {
            "type": "date",
            "copy_to": "created"
          },
          "created": {
            "type": "date"
          },
          "_updated": {
            "type": "date",
            "copy_to": "updated"
          },
          "cadi_id": {
            "type": "text",
            "analyzer": "lowercase_whitespace_analyzer"
          },
          "dataset": {
            "type": "text",
            "analyzer": "lowercase_whitespace_analyzer"
          },
          "trigger": {
            "type": "text",
            "analyzer": "lowercase_whitespace_analyzer"
          },
          "ananote": {
            "type": "text",
            "analyzer": "lowercase_whitespace_analyzer"
          },
          "basic_info": {
            "type": "object",
            "properties": {
              "cadi_id": {
                "type": "keyword",
                "copy_to": "cadi_id"
              },
              "ana_notes": {
                "type": "text",
                "copy_to": "ananote",
                "analyzer": "lowercase_whitespace_analyzer"
              },
              "created": {
                "type": "date"
              }
            }
          },
          "input_data": {
            "type": "object",
            "properties": {
              "primary_datasets": {
                "type": "object",
                "properties": {
                  "path": {
                    "type": "text",
                    "copy_to": "dataset",
                    "analyzer": "lowercase_whitespace_analyzer"
                  },
                  "year": {
                    "type": "keyword"
                  },
                  "triggers": {
                    "type": "object",
                    "properties": {
                      "trigger": {
                        "type": "text",
                        "analyzer": "lowercase_whitespace_analyzer",
                        "copy_to": "trigger"
                      }
                    }
                  }
                }
              },
              "mc_sig_datasets": {
                "type": "text",
                "copy_to": "dataset",
                "analyzer": "lowercase_whitespace_analyzer"
              },
              "mc_bg_datasets": {
                "type": "text",
                "copy_to": "dataset",
                "analyzer": "lowercase_whitespace_analyzer"
              },
              "json_files": {
                "type": "text",
                "analyzer": "lowercase_whitespace_analyzer"
              }
            }
          },
          "cadi_status": {
            "type": "text",
            "analyzer": "lowercase_whitespace_analyzer"
          },
          "cadi_info": {
            "type": "object",
            "properties": {
              "status": {
                "type": "keyword",
                "copy_to": "cadi_status"
              },
              "contact_person": {
                "type": "text",
                "copy_to": "researcher"
              },
              "publication_status": {
                "type": "text",
                "analyzer": "lowercase_whitespace_analyzer"
              }
            }
          },
          "additional_resources": {
            "type": "object",
            "properties": {
              "documentations": {
                "type": "text",
                "analyzer": "lowercase_whitespace_analyzer",
                "copy_to": "keyword"
              },
              "keywords": {
                "type": "text",
                "analyzer": "lowercase_whitespace_analyzer",
                "copy_to": "keyword"
              }
            }
          },
          "main_measurements": {
            "type": "nested",
            "properties": {
              "signal_event_selection": {
                "type": "object",
                "properties": {
                  "physics_objects": {
                    "type": "nested",
                    "properties": {
                      "object": {
                        "type": "keyword",
                        "copy_to": "object"
                      },
                      "pt_cut": {
                        "type": "keyword",
                        "copy_to": "pt"
                      },
                      "pt_cut_min": {
                        "type": "keyword",
                        "copy_to": "pt_min"
                      },
                      "pt_cut_max": {
                        "type": "keyword",
                        "copy_to": "pt_max"
                      },
                      "eta_cut": {
                        "type": "keyword",
                        "copy_to": "eta"
                      },
                      "eta_cut_min": {
                        "type": "keyword",
                        "copy_to": "eta_min"
                      },
                      "eta_cut_max": {
                        "type": "keyword",
                        "copy_to": "eta_max"
                      },
                      "electron_type": {
                        "type": "keyword",
                        "copy_to":
                          "main_measurements.signal_event_selection.physics_objects.object_type"
                      },
                      "muon_type": {
                        "type": "keyword",
                        "copy_to":
                          "main_measurements.signal_event_selection.physics_objects.object_type"
                      },
                      "tau_type": {
                        "type": "keyword",
                        "copy_to":
                          "main_measurements.signal_event_selection.physics_objects.object_type"
                      },
                      "jet_type": {
                        "type": "keyword",
                        "copy_to":
                          "main_measurements.signal_event_selection.physics_objects.object_type"
                      },
                      "photon_type": {
                        "type": "keyword",
                        "copy_to":
                          "main_measurements.signal_event_selection.physics_objects.object_type"
                      },
                      "met_type": {
                        "type": "keyword",
                        "copy_to":
                          "main_measurements.signal_event_selection.physics_objects.object_type"
                      },
                      "track_type": {
                        "type": "keyword",
                        "copy_to":
                          "main_measurements.signal_event_selection.physics_objects.object_type"
                      },
                      "number": {
                        "type": "object",
                        "properties": {
                          "sign": {
                            "type": "keyword"
                          },
                          "number": {
                            "type": "keyword"
                          }
                        }
                      }
                    }
                  },
                  "veto": {
                    "type": "nested",
                    "properties": {
                      "object": {
                        "type": "keyword",
                        "copy_to": "veto"
                      },
                      "pt_cut": {
                        "type": "keyword",
                        "copy_to": "veto_pt"
                      },
                      "pt_cut_min": {
                        "type": "keyword",
                        "copy_to": "veto_pt_min"
                      },
                      "pt_cut_max": {
                        "type": "keyword",
                        "copy_to": "veto_pt_max"
                      },
                      "eta_cut": {
                        "type": "keyword",
                        "copy_to": "veto_eta"
                      },
                      "eta_cut_min": {
                        "type": "keyword",
                        "copy_to": "veto_eta_min"
                      },
                      "eta_cut_max": {
                        "type": "keyword",
                        "copy_to": "veto_eta_max"
                      },
                      "electron_type": {
                        "type": "keyword",
                        "copy_to":
                          "main_measurements.signal_event_selection.veto.object_type"
                      },
                      "muon_type": {
                        "type": "keyword",
                        "copy_to":
                          "main_measurements.signal_event_selection.veto.object_type"
                      },
                      "tau_type": {
                        "type": "keyword",
                        "copy_to":
                          "main_measurements.signal_event_selection.veto.object_type"
                      },
                      "jet_type": {
                        "type": "keyword",
                        "copy_to":
                          "main_measurements.signal_event_selection.veto.object_type"
                      },
                      "photon_type": {
                        "type": "keyword",
                        "copy_to":
                          "main_measurements.signal_event_selection.veto.object_type"
                      },
                      "met_type": {
                        "type": "keyword",
                        "copy_to":
                          "main_measurements.signal_event_selection.veto.object_type"
                      },
                      "track_type": {
                        "type": "keyword",
                        "copy_to":
                          "main_measurements.signal_event_selection.veto.object_type"
                      },
                      "number": {
                        "type": "object",
                        "properties": {
                          "sign": {
                            "type": "keyword"
                          },
                          "number": {
                            "type": "keyword"
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "deposit_options": {
    "ui:object": "tabView",
    "ui:options": {
      "display": "grid",
      "full": true,
      "view": {
        "vertical": true,
        "sidebarColor": "grey-5-a"
      }
    },
    "statistical_treatment": {
      "combine_datacard": {
        "ui:field": "CapFiles"
      },
      "statistics_questionnaire": {
        "ui:options": {
          "query": "/api/deposits/?type=cms-questionnaire-v0.0.1"
        },
        "ui:field": "ImportDataField"
      }
    },
    "auxiliary_measurements": {
      "items": {
        "ui:order": ["type", "links"],
        "links": {
          "items": {
            "ui:field": "CapFiles"
          }
        }
      }
    },
    "main_measurements": {
      "ui:array": "AccordionArrayField",
      "items": {
        "ui:order": [
          "description",
          "detailed_desciption",
          "analysis_code",
          "signal_event_selection",
          "processing_steps",
          "*"
        ],
        "signal_event_selection": {
          "veto": {
            "items": {
              "ui:order": ["number", "object", "pt_cut", "eta_cut", "*"]
            }
          },
          "physics_objects": {
            "items": {
              "ui:order": [
                "number",
                "object",
                "sel_criteria",
                "pt_cut",
                "pt_cut_min",
                "pt_cut_max",
                "eta_cut",
                "eta_cut_min",
                "eta_cut_max",
                "*"
              ],
              "object": {
                "ui:widget": "select"
              }
            }
          }
        },
        "processing_steps": {
          "items": {
            "ui:order": [
              "configuration_file",
              "run_instructions",
              "output_data"
            ],
            "output_data": {
              "ui:field": "CapFiles"
            },
            "run_instructions": {
              "ui:field": "CapFiles"
            },
            "configuration_file": {
              "ui:field": "CapFiles"
            }
          }
        },
        "analysis_code": {
          "ui:field": "CapFiles"
        }
      }
    },
    "basic_info": {
      "ui:order": ["cadi_id", "abstract", "conclusion", "*"],
      "cadi_id": {
        "ui:options": {
          "autofill_from": "/api/cms/cadi/",
          "autofill_fields": [
            [["paper"], ["cadi_info", "paper"]],
            [["name"], ["cadi_info", "name"]],
            [["description"], ["cadi_info", "description"]],
            [["contact"], ["cadi_info", "contact"]],
            [["created"], ["cadi_info", "created"]],
            [["twiki"], ["cadi_info", "twiki"]],
            [["pas"], ["cadi_info", "pas"]],
            [["publication_status"], ["cadi_info", "publication_status"]],
            [["status"], ["cadi_info", "status"]]
          ]
        }
      },
      "abstract": {
        "ui:widget": "textarea"
      },
      "ana_notes": {
        "ui:array": "StringArrayField",
        "ui:options": {
          "size": "full",
          "display": "grid"
        }
      }
    },
    "analysis_reuse_mode": {
      "ui:widget": "switch"
    },
    "input_data": {
      "mc_sig_datasets": {
        "items": {
          "ui:options": {
            "suggestions": "/api/cms/datasets?query="
          }
        },
        "ui:array": "StringArrayField"
      },
      "json_files": {
        "items": {
          "ui:field": "CapFiles"
        }
      },
      "mc_bg_datasets": {
        "items": {
          "ui:options": {
            "suggestions": "/api/cms/datasets?query="
          }
        },
        "ui:array": "StringArrayField"
      },
      "ui:order": [
        "primary_datasets",
        "mc_sig_datasets",
        "mc_bg_datasets",
        "json_files",
        "*"
      ],
      "primary_datasets": {
        "items": {
          "ui:order": ["path", "ranges", "year", "triggers"],
          "ui:options": {
            "stringify": ["path"]
          },
