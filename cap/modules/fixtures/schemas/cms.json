{
  "name": "cms-analysis",
  "version": "0.0.1",
  "fullname": "CMS Analysis",
  "experiment": "CMS",
  "is_indexed": true,
  "use_deposit_as_record": true,
  "deposit_schema": {
    "title": "CMS Analysis - Draft",
    "dependencies": {
      "analysis_reuse_mode": {
        "properties": {
          "main_measurements": {
            "items": {
              "$ref":
                "https://analysispreservation.cern.ch/schemas/deposits/records/cms-main-measurements-v0.0.1.json"
            },
            "title": "Main Measurements Workflows",
            "type": "array",
            "description":
              "Please provide information about the main measurements of your analysis",
            "id": "main_measurements"
          },
          "final_results": {
            "title": "Final Results",
            "type": "object",
            "properties": {
              "code_base": {
                "x-cap-file": {},
                "type": "string",
                "description": "Include your code for final results",
                "title": "Your Code"
              },
              "processing_steps": {
                "$ref":
                  "https://analysispreservation.cern.ch/schemas/deposits/records/cms-processing-v0.0.1.json"
              }
            },
            "description":
              "Please provide information necessary to generate final plots and tables for your analysis."
          },
          "systematic_uncertainties": {
            "items": {
              "type": "object",
              "properties": {
                "type": {
                  "enum": [
                    "associated_to_obj",
                    "associated_to_background_pred_methods",
                    "associated_to_mc_production",
                    "assocciated_to_luminosity",
                    "statement_about_most_important_systematics"
                  ],
                  "type": "string",
                  "title": "Type"
                },
                "physics_object": {
                  "type": "string",
                  "description":
                    "Choose from objects defined in main measurements",
                  "title": "Physics Object"
                },
                "statement": {
                  "type": "string",
                  "title": "Statement about the most important systematics"
                }
              }
            },
            "type": "array",
            "description": "Details on the systematic uncertainties",
            "title": "Systematic Uncertainties"
          },
          "auxiliary_measurements": {
            "items": {
              "type": "object",
              "properties": {
                "type": {
                  "enum": [
                    "trigger_efficiencies",
                    "background_measurements",
                    "systematic_uncertainties",
                    "mc_scale_factors",
                    "object_efficiencies",
                    "rates",
                    "acceptance_corrections",
                    "other"
                  ],
                  "type": "string",
                  "title": "Type"
                },
                "links": {
                  "items": {
                    "x-cap-file": {},
                    "type": "string"
                  },
                  "type": "array",
                  "description": "Code for auxiliary measurements",
                  "title": "Your code"
                }
              }
            },
            "type": "array",
            "description":
              "Provide details on auxiliary measurements used in the analysis",
            "title": "Auxiliary Measurements"
          },
          "background_estimations": {
            "items": {
              "$ref":
                "https://analysispreservation.cern.ch/schemas/deposits/records/cms-background-estimation-v0.0.1.json"
            },
            "type": "array",
            "description": "Details on the background estimation methods",
            "title": "Background Estimation"
          }
        }
      }
    },
    "additionalProperties": true,
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
      "_buckets": {
        "$ref":
          "https://analysispreservation.cern.ch/schemas/deposits/records/buckets-v0.0.1.json"
      },
      "control_number": {
        "type": "string"
      },
      "_files": {
        "$ref":
          "https://analysispreservation.cern.ch/schemas/deposits/records/file-v0.0.1.json"
      },
      "basic_info": {
        "properties": {
          "cadi_id": {
            "pattern": "^[A-Z0-9]{3}-[0-9]{2}-[0-9]{3}$",
            "type": "string",
            "x-validate-cadi-id": {},
            "placeholder": "e.g. JME-10-107",
            "title": "CADI ID",
            "description": "e.g. JME-10-107"
          },
          "analysis_keywords": {
            "type": "object",
            "properties": {
              "collision_system": {
                "type": "array",
                "uniqueItems": true,
                "items": {
                  "type": "string",
                  "enum": [
                    "p-p",
                    "p-Pb",
                    "Pb-Pb",
                    "Xe-Xe",
                    "Other (incl. Cosmic Rays)"
                  ]
                },
                "description": "(will be ORed inside this category)",
                "title": "Collision system"
              },
              "accelerator_parameters": {
                "type": "array",
                "uniqueItems": true,
                "items": {
                  "type": "string",
                  "enum": [
                    "900 GeV",
                    "2.76 TeV",
                    "5.02 TeV",
                    "5.44 TeV",
                    "5.5 TeV",
                    "7 TeV",
                    "8 TeV",
                    "8.16 TeV",
                    "8.8 TeV",
                    "13 TeV",
                    "14 TeV",
                    "27 TeV"
                  ]
                },
                "description": "(will be ORed inside this category)",
                "title": "Accelerator Parameters"
              },
              "physics_theme": {
                "type": "array",
                "uniqueItems": true,
                "items": {
                  "type": "string",
                  "enum": [
                    "B-physics and similar",
                    "Standard Model",
                    "Searches for physics beyond the Standard Model",
                    "Heavy Ion Physics",
                    "Upgrade studies",
                    "Physics objects and Detector performance"
                  ]
                },
                "title": "Physics Theme"
              },
              "sm_analysis_characteristics": {
                "type": "array",
                "uniqueItems": true,
                "items": {
                  "type": "string",
                  "enum": [
                    "Rare Production and Decays",
                    "Measurement",
                    "Cross Section",
                    "Mass and/or width",
                    "Couplings and/or properties",
                    "Spectroscopy"
                  ]
                },
                "description": "(will be ORed inside this category)",
                "title": "SM: Analysis Characteristics"
              },
              "interpretation": {
                "type": "array",
                "uniqueItems": true,
                "items": {
                  "type": "string",
                  "enum": [
                    "Simplified Model Spectrum",
                    "Effective Field Theory",
                    "Standard Model Fits inc. PDFs",
                    "Generator and simulation tuning",
                    "Combination"
                  ]
                },
                "description": "(will be ORed inside this category)",
                "title": "Interpretation"
              },
              "final_states": {
                "type": "array",
                "uniqueItems": true,
                "items": {
                  "type": "string",
                  "enum": [
                    "B-hadrons",
                    "C-hadrons",
                    "Tracks",
                    "Jets: 1 jet",
                    "Jets: 2 jets",
                    "Jets: >=3 jets",
                    "Jets: all hadronic",
                    "Jets: Boosted/substructure",
                    "Jets: B-jets",
                    "Jets: C-jets",
                    "Jets: Forward jets",
                    "Z boson",
                    "W boson",
                    "di-boson",
                    "tri- and multi bosons",
                    "Higgs bosons",
                    "Top",
                    "Single lepton",
                    "Muon",
                    "Electron",
                    "Tau",
                    "Photon",
                    "Di-lepton; opposite charge",
                    "Di-lepton; same charge",
                    ">=3 leptons",
                    "Missing energy/invisible",
                    "Tagged proton",
                    "Long-lived signature",
                    "Detector performance",
                    "ttbar+(anything but Higgs)",
                    "HH",
                    "ttbar+Higgs boson",
                    "Option 33"
                  ]
                },
                "description": "(will be ORed inside this category)",
                "title": "Final states"
              },
              "further_search_categorisation": {
                "type": "array",
                "uniqueItems": true,
                "items": {
                  "type": "string",
                  "enum": [
                    "Supersymmetry",
                    "Supersymmetry: GMSB",
                    "Supersymmetry: RPV",
                    "Supersymmetry: Squarks and gluinos",
                    "Supersymmetry: Stops and sbottoms",
                    "Supersymmetry: Charginos; neutralinos; winos; higgsinos",
                    "Supersymmetry: Sleptons",
                    "New Resonances",
                    "Extra Dimensions",
                    "Dark Matter & Dark sector",
                    "Additional Higgs bosons",
                    "Contact Interactions",
                    "Lepton flavour violation",
                    "Leptoquarks",
                    "Long-lived particles",
                    "Compositeness",
                    "Excited fermions",
                    "Heavy additional fermions"
                  ]
                },
                "description": "(will be ORed inside this category)",
                "title": "Further search categorisation"
              },
              "further_categorisation_heavy_ion": {
                "type": "array",
                "uniqueItems": true,
                "items": {
                  "type": "string",
                  "enum": [
                    "Particle distributions and correlations",
                    "Open Heavy flavor",
                    "Quarkonia",
                    "Jet properties",
                    "(Elliptic) flow and event properties",
                    "Forward physics",
                    "Ultra-peripheral collisions",
                    "Initial stage"
                  ]
                },
                "description": "(will be ORed inside this category)",
                "title": "Further categorisation Heavy Ion results"
              }
            },
            "id": "analysis_keywords",
            "title": "Analysis Keywords"
          },
          "abstract": {
            "type": "string",
            "title": "Abstract"
          },
          "ana_notes": {
            "items": {
              "pattern": "^AN-[0-9]{4}/[0-9]{3}$",
              "placeholder": "e.g. AN-2010-107",
              "type": "string"
            },
            "type": "array",
            "title": "Analysis Notes"
          },
          "conclusion": {
            "type": "string",
            "title": "Conclusion"
          }
        },
        "title": "Basic Information",
        "type": "object",
        "id": "basic_info",
        "required": ["cadi_id"],
        "description":
          "Please provide some information relevant for all parts of the Analysis here"
      },
      "analysis_reuse_mode": {
        "type": "string",
        "description":
          "please turn this mode on if you want to capture additional information about main and auxiliary measurements, systematic uncertainties, background estimates, final state particles",
        "title": "Full reproducibility mode"
      },
      "input_data": {
        "properties": {
          "mc_bg_datasets": {
            "items": {
              "placeholder": "Path",
              "x-validate-das-path": {},
              "type": "string"
            },
            "title": "Monte Carlo Background Datasets",
            "type": "array",
            "description": "Please list all Monte Carlo Background datasets",
            "id": "mc_bg_datasets"
          },
          "json_files": {
            "items": {
              "x-cap-file": {},
              "type": "string"
            },
            "type": "array",
            "title": "Official JSON files"
          },
          "primary_datasets": {
            "items": {
              "type": "object",
              "x-validate-cms-trigger": {},
              "properties": {
                "path": {
                  "type": "string",
                  "x-validate-das-path": {},
                  "title": "Path"
                }
              },
              "dependencies": {
                "path": {
                  "properties": {
                    "year": {
                      "type": "number",
                      "title": "Dataset Year",
                      "description": "Select dataset year and then triggers used",
                      "enum": [2007, 2008, 2009, 2010, 2011, 2012, 2013, 2014, 2015, 2016, 2017, 2018, 2019, 2020, 2021 ]
                    }
                  },
                  "dependencies": {
                    "year": {
                      "properties": {
                        "triggers": {
                          "items": {
                            "type": "object",
                            "properties": {
                              "trigger": {
                                "type": "string",
                                "title": "Trigger"
                              }
                            }
                          },
                          "title": "Triggers",
                          "type": "array",
                          "description": "Add selection triggers here",
                          "id": "triggers"
                        }
                      }
                    }
                  }
                }
              },
              "required": ["path"]
            },
            "title": "Primary Datasets",
            "type": "array",
            "description": "Please list all primary datasets here",
            "id": "primary_datasets"
          },
          "mc_sig_datasets": {
            "items": {
              "placeholder": "Path",
              "x-validate-das-path": {},
              "type": "string"
            },
            "title": "Monte Carlo Signal Datasets",
            "type": "array",
            "description": "Please list all Monte Carlo Signal datasets",
            "id": "mc_sig_datasets"
          }
        },
        "title": "Input Data",
        "type": "object",
        "id": "input_data",
        "description":
          "Please list all datasets and triggers relevant for your analysis here"
      },
      "cadi_info": {
        "$ref":
          "https://analysispreservation.cern.ch/schemas/deposits/records/cms-cadi-info-v0.0.1.json"
      },
      "ntuples_production": {
        "items": {
          "properties": {
            "code_base": {
              "x-cap-file": {},
              "type": "string",
              "description": "Include your code for n-tuple production",
              "title": "Your Code"
            },
            "processing_steps": {
              "$ref":
                "https://analysispreservation.cern.ch/schemas/deposits/records/cms-processing-v0.0.1.json"
            }
          },
          "type": "object",
          "description": "Add instructions to run your code",
          "title": ""
        },
        "title": "N-tuples Production",
        "type": "array",
        "description":
          "Provide details on the intermediate n-tuples production",
        "id": "ntuples_production"
      },
      "_access": {
        "$ref":
          "https://analysispreservation.cern.ch/schemas/deposits/records/access-v0.0.1.json"
      },
      "_deposit": {
        "$ref":
          "https://analysispreservation.cern.ch/schemas/deposits/records/deposit-v0.0.1.json"
      },
      "additional_resources": {
        "title": "Additional Resources",
        "type": "object",
        "properties": {
          "comments": {
            "type": "string",
            "title": "Additional comment"
          },
          "presentations": {
            "items": {
              "type": "object",
              "properties": {
                "conference": {
                  "type": "string",
                  "title": "Conference/meeting"
                },
                "items": {
                  "x-cap-file": {},
                  "type": "string",
                  "title": "Presentation"
                }
              }
            },
            "title": "Presentations",
            "type": "array",
            "id": "presentations",
            "description": "Upload presentations about the analysis"
          },
          "documentations": {
            "items": {
              "x-cap-file": {},
              "type": "string"
            },
            "title": "Additional Documentations",
            "type": "array",
            "id": "documentations",
            "description": "Add any useful additional documentation"
          },
          "publications": {
            "items": {
              "$ref":
                "https://analysispreservation.cern.ch/schemas/deposits/records/publication-v0.0.1.json"
            },
            "title": "Publications",
            "type": "array",
            "id": "publications",
            "description": "Add references to related publications"
          },
          "keywords": {
            "type": "string",
            "id": "keywords",
            "title": "Keywords"
          },
          "internal_discussions": {
            "items": {
              "type": "string",
              "title": "URL"
            },
            "title": "Internal Discussions",
            "type": "array",
            "id": "internal-discussions",
            "description": "Link to hypernews thread"
          }
        },
        "description": "Add any useful additional documentation on the analysis"
      },
      "$ana_type": {
        "type": "string"
      },
      "$schema": {
        "type": "string"
      },
      "general_title": {
        "type": "string"
      },
      "_experiment": {
        "type": "string"
      },
      "_fetched_from": {
        "type": "string"
      },
      "_user_edited": {
        "type": "boolean"
      }
    }
  },
  "deposit_mapping": {
    "settings": {
      "analysis": {
        "analyzer": {
          "lowercase_whitespace_analyzer": {
            "type": "custom",
            "tokenizer": "whitespace",
            "filter": ["lowercase"]
          }
        }
      }
    },
    "mappings": {
