{
  "experiment": "ATLAS",
  "use_deposit_as_record": true,
  "version": "0.0.1",
  "is_indexed": false,
  "fullname": "",
  "deposit_schema": {
    "title": "Simple Interpolated Publisher",
    "required": ["publisher_type", "publish"],
    "additionalProperties": false,
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
      "publisher_type": {
        "enum": ["interpolated-pub"],
        "type": "string"
