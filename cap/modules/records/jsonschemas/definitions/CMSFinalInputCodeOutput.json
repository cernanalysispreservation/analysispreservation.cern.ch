{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Processing",
  "type": "array",
  "default": [{}],
  "items": {
    "title": "Input",
    "type": "object",
    "properties": {
      "input_data": {
        "title": "Input",        
        "type": "object",
        "properties": {
          "dataset": {
            "title": "Dataset",
            "type": "string",
            "enum": []
          }
        }
      },
      "user_code": {
        "title": "Produced With",
        "type": "object",
        "properties": {
          "config_files": {
            "title": "URL",
            "type": "string"
          }
        }
      },
      "run_instructions": {
        "title": "Run Instructions",
        "description": "Provide run instructions",
        "type": "object",
        "properties": {
          "type": {
            "title": "Type",
            "type": "string",
            "enum": ["readme", "makefile", "upload"]
          }
        }
      },
      "output_data": {
        "title": "Output",        
        "type": "object",
        "properties": {
          "output_url": {
            "title": "URL",
            "type": "string"
          }
        }
      },
	  "datacard": {
		"title": "Combine Datacard",        
        "type": "string"
	  }
    }
  }
}
