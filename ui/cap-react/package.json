{
  "name": "cap-react",
  "version": "0.3.0",
  "description": "CERN Analysis Preservation UI",
  "engines": {
    "npm": ">=3"
  },
  "scripts": {
    "preinstall": "node tools/nodeVersionCheck.js",
    "precommit": "pretty-quick --staged --single-quote",
    "remove-demo": "babel-node tools/removeDemo.js",
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm run start-message",
    "start": "concurrently -k -r -s first \"npm run test:watch\" \"npm run open:src\"",
    "lint": "eslint src",
    "lint:fix": "eslint --fix",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run lint && npm run test:run && npm run clean-dist",
    "build": "vite build",
    "test": "vitest",
    "test:run": "vitest run",
    "test:cover": "npm run test -- --coverage ",
    "test:cover:CI": "npm run test:CI -- --coverage && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "test:e2e": "cypress open",
    "test:e2e:run": "cypress run --headless --browser chrome",
    "tests": "start-server-and-test start http://localhost:3000 test:e2e:run",
    "open:cover": "npm run test:cover && opn ./coverage/lcov-report/index.html",
    "dev": "vite",
    "dev:docker": "DOCKER=true vite",
    "preview": "vite preview"
  },
  "author": "Pamfilos Fokianos",
  "license": "MIT",
  "dependencies": {
    "@ant-design/icons": "4.7.0",
